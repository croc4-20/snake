{
  "name": "slither",
  "version": "1.0.0",
  "description": "slither game",
  "main": "",
  "scripts": {
    "clean": "rimraf build",
    "start": "npm run build && npm run copy-config && NODE_ENV=production node server.js",
    "dev": "webpack serve --open --config webpack.slither.config.mjs.js",
    "heroku-postbuild": "npm install --legacy-peer-deps && npm run build",
    "build": "tsc && webpack --config webpack.slither.config.mjs.js && npm run copy-server",
    "release": "webpack --config webpack.slither.config.mjs.js --mode production",
    "copy-server": "copyfiles server.js build/server",
    "copy-config": "copyfiles -u 1 'web/common/*.js' build/web/common 'web/element/*.js' build/web/element 'web/framework/*.js' build/web/framework 'web/libs/*.js' build/web/libs"
  },
  "type": "module",
  "keywords": [
    "game",
    "slither",
    "typescript"
  ],
  "alias": {
    "~": "./"
  },
  "dependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/es6-shim": "^0.31.45",
    "@types/ip": "^0.0.31",
    "@types/node": "^22.7.4",
    "@types/socket.io": "^3.0.2",
    "@types/stats.js": "^0.17.0",
    "@types/webpack-env": "^1.18.5",
    "@types/ws": "^3.2.0",
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "babel-loader": "^9.2.1",
    "assert": "^2.1.0",
    "buffer": "^5.0.8",
    "chokidar": "^2.0.4",
    "concurrently": "^3.6.1",
    "copyfiles": "^2.4.1",
    "es6-shim": "^0.35.8",
    "eventemitter3": "^2.0.2",
    "express": "^4.18.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "ip": "^1.1.5",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "ts-loader": "^9.5.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "ws": "^3.3.2"
  },
  "author": "wanghx",
  "license": "MIT",
  "devDependencies": {
    
    "cross-env": "^7.0.3",
    "eslint": "^7.x.x",
    "nodemon": "^1.18.3",
    "opn": "^5.1.0",
    "socket.io": "^4.8.0",
    "ts-node": "^10.9.2",
    "tslint": "^5.8.0",
    "webpack-dev-server": "^5.1.0"
  }
}
